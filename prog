pushstr "type nuber: "
print

readnum
popvar n

defsub even
	pushstr "even\n"
	print

	pushvar n
	pushnum 2
	div
	popvar n

	return
endsub

defsub odd
	pushstr "odd\n"
	print

	pushnum 1
	pushvar n
	pushnum 3
	mul
	add
	popvar n

	return
endsub

pushnum 0
popvar iter

while
	pushvar n
	pushnum 1
	neq
do
	pushstr "\n"
	pushvar n
	print
	print

	if
		pushvar n
		pushnum 2
		mod
	then
		gosub odd
	else
		gosub even
	endif

	pushvar iter
	pushnum 1
	add
	popvar iter
endwhile

pushstr "\n"
pushvar iter
pushstr "iter="
print
print
print

